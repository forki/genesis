<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
        <!-- Name Generation -->
        <add key="minimumNameLength" value="3" />

        <!-- Midpoint displacement -->
        <add key="startingSpread" value="0.3" />
        <add key="spreadReduction" value="0.5" />

        <!-- Value Noise -->
        <add key="dimming" value="0.25" />
        <add key="minLight" value="0.05" />
        <add key="lightBoost" value="1.2" />
    </appSettings>
</configuration>